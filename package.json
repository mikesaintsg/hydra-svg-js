{
	"name": "hydra-svg-js",
	"version": "0.0.36",
	"description": "Hydrate SVG tags with icons from your favorite libraries",
	"type": "module",
	"main": "dist/main.cjs",
	"module": "dist/main.mjs",
	"browser": "dist/main.js",
	"exports": {
		".": {
			"import": "./dist/main.mjs",
			"require": "./dist/main.cjs"
		}
	},
	"bin": {
		"hydra": "bin/cli.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dist": "rollup -c config/rollup.config.js",
		"hydra": "node bin/cli.js",
		"lint": "eslint .",
		"test": "mocha \"./test/**/**.test.js\" --recursive --require @babel/register",
		"cover": "nyc --reporter=text yarn test"
	},
	"license": "MIT",
	"author": "Michael Garcia",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hydra-svg-js/hydra-svg-js.git"
	},
	"bugs": {
		"url": "https://github.com/hydra-svg-js/hydra-svg-js/issues"
	},
	"homepage": "https://github.com/hydra-svg-js/hydra-svg-js#readme",
	"keywords": [
		"hydra-svg",
		"hyrdra",
		"hydrate",
		"svg",
		"icons",
		"heroicons",
		"fontawesome"
	],
	"devDependencies": {
		"@babel/cli": "^7.14.3",
		"@babel/core": "^7.14.3",
		"@babel/preset-env": "^7.14.4",
		"@babel/register": "^7.13.16",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^19.0.0",
		"@rollup/plugin-node-resolve": "^13.0.0",
		"chai": "^4.3.4",
		"core-js": "^3.13.1",
		"eslint": "^7.27.0",
		"jsdom": "^16.6.0",
		"mocha": "^8.4.0",
		"nyc": "^15.1.0",
		"rollup": "^2.51.0",
		"rollup-plugin-terser": "^7.0.2"
	},
	"dependencies": {
		"commander": "^7.2.0",
		"hydra-generator": "^0.0.4",
		"hydra-purge": "^0.0.6"
	}
}
